Shortcuts

alias ..='cd ..'
alias c='clear'
alias ll='ls -la'
alias grep='grep --color=auto'
alias diff='colordiff'
alias ipt='sudo /sbin/iptables'
alias ping='ping -c 5'
alias fastping='ping -c 100 -s.2'
alias ports='netstat -tulanp'
alias df='df -h'
alias cpu='top -o cpu'
alias mem='top -o rsize'
alias gp='git pull'
